<span class="sd">    &gt;&gt;&gt; from simpleparser import token, option</span>
<span class="sd">    &gt;&gt;&gt; p = option(token(&#39;foo&#39;))</span>
<span class="sd">    &gt;&gt;&gt; p.exec(&#39;foobar&#39;)</span>
